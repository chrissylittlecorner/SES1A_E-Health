<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3">
          <i class="fas fa-user-plus"></i> Doctor Registration
        </h1>
        <%- include('./partials/errors') %>
  
        <form action="/doctors/doctorregister" method="POST">
          <div class="form-group">
            <label for="dname"> Name</label>
            <input
              type="dname"
              id="dname"
              name="dname"
              class="form-control"
              placeholder="Enter Name"
        
            />
          </div>
          <div class="form-group">
            <label for="dqualification">Doctor Qualification</label>
            <input
              type="dqualification"
              id="dqualification"
              name="dqualification"
              class="form-control"
              placeholder="Enter Your Qualification"
              
            />
          </div>
          <div class="form-group">
            <label for="dlocation">Doctor Location</label>
            <input
              type="dlocation"
              id="dlocation"
              name="dlocation"
              class="form-control"
              placeholder="Enter Location"
              
            />
          </div>
          <div class="form-group">
            <label for="dpractice">Doctor Practice</label>
            <input
              type="dpractice"
              id="dpractice"
              name="dpractice"
              class="form-control"
              placeholder="Enter Practice"
             
            />
          </div>
          <div class="form-group">
            <label for="dmobileno">Mobile Number</label>
            <input
              type="dmobileno"
              id="dmobileno"
              name="dmobileno"
              class="form-control"
              placeholder="Enter Mobile Number"
              
            />
          </div>
          <div class="form-group">
            <label for="demail">Email</label>
            <input
              type="email"
              id="demail"
              name="demail"
              class="form-control"
              placeholder="Enter Email"
             
            />
          </div>
          <div class="form-group">
            <label for="dpassword">Password</label>
            <input
              type="password"
              id="dpassword"
              name="dpassword"
              class="form-control"
              placeholder="Create Password"
            
            />
          </div>
          <div class="form-group">
            <label for="dpassword2">Confirm Password</label>
            <input
              type="password"
              id="dpassword2"
              name="dpassword2"
              class="form-control"
              placeholder="Confirm Password"

            />
          </div>
          <button type="submit" class="btn btn-primary btn-block">
            Register
          </button>
        </form>
        <p class="lead mt-4">Have An Account? <a href="/doctors/doctorlogin">Login</a></p>
      </div>
    </div>
